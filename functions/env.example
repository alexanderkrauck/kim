# Local Development Environment Variables
# Copy this file to .env and fill in your actual API keys for local development

# OpenAI API Key
OPENAI_API_KEY=sk-your-openai-api-key-here

# Apollo.io API Key
APOLLO_API_KEY=your-apollo-api-key-here

# Apifi API Key
APIFI_API_KEY=your-apifi-api-key-here

# Perplexity API Key
PERPLEXITY_API_KEY=pplx-your-perplexity-api-key-here

# Firebase Project ID (for local testing)
FIREBASE_PROJECT_ID=your-firebase-project-id

# Development Settings
DEBUG=true
LOG_LEVEL=debug 